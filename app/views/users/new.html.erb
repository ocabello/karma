

<div class="create">
  <div class="container">
      
<%= form_for(@user) do |f| %>
  <h1>Sign Up</h1>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  
  <div class="field">
    <%= f.label :points %><br>
    <%= f.text_field :points %>
  </div>
  
  <div class="field">
    <%= f.label :testId %><br>
    <%= f.text_field :testId %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

</div></div>