

<div class="create">
  <div class="container">
      
<%= form_for(@post) do |f| %>
  <h1>New Post</h1>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>
 
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

</div></div>