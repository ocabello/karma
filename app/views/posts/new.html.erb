
<h2>Create a New Post!</h2>

<div class="create">
  <div class="container">
    
<%= render 'shared/error_messages' %>
<%= form_for(@post) do |f| %>
  <h1>New Post</h1>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title, class: 'form-control'%>
    <%= f.label :description %><br>
    <%= f.text_area :description, class: 'form-control' %>
  </div>
 
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

</div></div>