

<div class="create">
  <div class="container">
      
<%= form_for(@post) do |f| %>
  <%= render 'shared/error_messages' %>
  <h1>New Post</h1>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title, class: 'form-control'%>
    <%= f.label :description %><br>
    <%= f.text_field :description, class: 'form-control' %>
  </div>
 
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

</div></div>